<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.targil2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.targil2.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.targil2.Add_grades"
            android:label="@string/title_activity_add_grades" >
        </activity>
        <activity
            android:name="com.example.targil2.Sub_grades"
            android:label="@string/title_activity_sub_grades" >
        </activity>
        <activity
            android:name="com.example.targil2.ID_grades"
            android:label="@string/title_activity_id_grades" >
        </activity>
        <activity
            android:name="com.example.targil2.Avg_grades"
            android:label="@string/title_activity_avg_grades" >
        </activity>
    </application>

</manifest>